#!/bin/sh
#
# emote - prints emotes based on keywords
# https://github.com/kori/bin
#

if [ -n "$1" ]; then
  case "$1" in
    "inteli") emote='13Ôº¥4Ôº®8Ôº•  11Ôº∞12Ôº¨13Ôº•4Ôº°8Ôº≥9Ôºµ11Ôº≤12Ôº•  4ÔºØ8Ôº¶  11Ôº¢12Ôº•13Ôº©4ÔºÆ8Ôºß  11Ôº©12ÔºÆ13Ôº¥4Ôº•8Ôº¨  11Ôº©12ÔºÆ13Ôº≥4Ôº©8Ôº§9Ôº•11‚Ñ¢' ;;
    "bully") emote='Hahahahahahahaha How The Fuck Is Cyber Bullying Real Hahahaha Nigga Just Walk Away From The Screen Like Nigga Close Your Eyes Haha' ;;
    "cn") emote='[2citation needed]' ;;
    "love") emote='( 4‚úø  „Éª‚Äø-) ~  4‚ô°' ;;
    "unsame") emote='[4‚úò] Unsame' ;;
    "sadwave") emote='(¬¥„Éª~„Éª`)„Éé' ;;
    "awkward") emote='( Ôºõ„Éªœâ„Éª)' ;;
    "same") emote='[3‚úî] Same' ;;
    "wee") emote='„Éü(„ÄÄ „Éªœâ„Éª)' ;;
    "denko") emote='(¬¥„Éªœâ„Éª`)' ;;
    "shrug") emote="‚îê('~'; )‚îå" ;;
    "yay") emote='„ÉΩ(‚Ä¢„ÉÆ‚Ä¢ „ÉΩ)' ;;
    "wink") emote='( ¬¥„Éª‚Äø-)' ;;
    "ohboy") emote='·ïï( ·êõ )·ïó' ;;
    "what") emote='( ¬¥_„Çù`)' ;;
    "mad") emote='(‚áÄ‚Ä∏‚Üº‚Ä∂)' ;;
    *) exit 1 ;;
  esac
  echo $emote
else
  echo "inteli, bully, cn, love, unsame, sadwave, awkward, same, wee, denko, shrug, yay, wink, ohboy, what, mad"
