def_key "k"
	scroll_up

def_key "j"
	scroll_down

def_key "h"
	master_screen

def_key "h"
	previous_column

def_key "l"
	slave_screen 

def_key "l"
	next_column

def_key "ctrl-u"
	page_up

def_key "ctrl-d"
	page_down

def_key "d"
	delete_playlist_items

def_key "-"
	jump_to_parent_directory

def_key ";"
	show_lyrics

def_command "q" [deferred]
	quit

